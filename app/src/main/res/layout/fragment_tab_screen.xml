<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TabScreen" >

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="match_parent"
        android:background="@drawable/background" >

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="45dp"
            android:layout_marginTop="15dp"
            android:text="IOS系统"
            android:textAppearance="?android:attr/textAppearanceLarge" />

        <ImageButton
            android:id="@+id/ImageButton27"
            android:layout_width="140dp"
            android:layout_height="40dp"
            android:layout_alignTop="@+id/imageButton27"
            android:layout_marginLeft="52dp"
            android:layout_toRightOf="@+id/imageButton27"
            android:background="@drawable/btn_ipad_off" />

        <ImageButton
            android:id="@+id/ImageButton29"
            android:layout_width="140dp"
            android:layout_height="40dp"
            android:layout_alignLeft="@+id/ImageButton27"
            android:layout_alignTop="@+id/ImageButton28"
            android:background="@drawable/btn_iphone_off" />

        <ImageButton
            android:id="@+id/ImageButton30"
            android:layout_width="140dp"
            android:layout_height="40dp"
            android:layout_alignTop="@+id/ImageButton29"
            android:layout_marginLeft="52dp"
            android:layout_toRightOf="@+id/ImageButton29"
            android:background="@drawable/btn_iphone_off" />

        <ImageButton
            android:id="@+id/ImageButton31"
            android:layout_width="140dp"
            android:layout_height="40dp"
            android:layout_alignTop="@+id/ImageButton30"
            android:layout_marginLeft="52dp"
            android:layout_toRightOf="@+id/ImageButton30"
            android:background="@drawable/btn_iphone_off" />

        <ImageButton
            android:id="@+id/ImageButton34"
            android:layout_width="140dp"
            android:layout_height="40dp"
            android:layout_alignTop="@+id/ImageButton33"
            android:layout_toLeftOf="@+id/ImageButton30"
            android:background="@drawable/btn_iphone_off" />

        <TextView
            android:id="@+id/TextView02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/textView4"
            android:layout_below="@+id/imageButton27"
            android:layout_marginTop="22dp"
            android:text="Android系统"
            android:textAppearance="?android:attr/textAppearanceLarge" />

        <ImageButton
            android:id="@+id/ImageButton28"
            android:layout_width="140dp"
            android:layout_height="40dp"
            android:layout_alignLeft="@+id/imageButton27"
            android:layout_below="@+id/TextView02"
            android:layout_marginTop="18dp"
            android:background="@drawable/btn_iphone_off" />

        <ImageButton
            android:id="@+id/ImageButton32"
            android:layout_width="140dp"
            android:layout_height="40dp"
            android:layout_alignLeft="@+id/ImageButton28"
            android:layout_below="@+id/ImageButton28"
            android:layout_marginTop="21dp"
            android:background="@drawable/btn_iphone_off" />

        <TextView
            android:id="@+id/TextView03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/TextView02"
            android:layout_below="@+id/ImageButton32"
            android:layout_marginTop="21dp"
            android:text="Android4.2以下系统"
            android:textAppearance="?android:attr/textAppearanceLarge" />

        <ImageButton
            android:id="@+id/imageButton27"
            android:layout_width="140dp"
            android:layout_height="40dp"
            android:layout_alignRight="@+id/TextView03"
            android:layout_below="@+id/textView4"
            android:layout_marginRight="22dp"
            android:layout_marginTop="18dp"
            android:background="@drawable/btn_iphone_off" />

        <ImageButton
            android:id="@+id/ImageButton33"
            android:layout_width="140dp"
            android:layout_height="40dp"
            android:layout_alignLeft="@+id/ImageButton32"
            android:layout_below="@+id/TextView03"
            android:layout_marginTop="18dp"
            android:background="@drawable/btn_iphone_off" />

        <ImageButton
            android:id="@+id/imgBtn_sf"
            android:layout_width="140dp"
            android:layout_height="40dp"
            android:layout_alignLeft="@+id/ImageButton31"
            android:layout_below="@+id/ImageButton34"
            android:background="@drawable/btn_reset_off" />

    </RelativeLayout>

</FrameLayout>